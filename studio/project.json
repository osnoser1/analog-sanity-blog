{
  "name": "studio",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "studio/src",
  "tags": [],
  "// targets": "to see all targets run: nx show project studio --web",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "sanity build",
        "cwd": "studio"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "sanity dev",
        "cwd": "studio"
      }
    }
  }
}
