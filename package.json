{
  "name": "@analog-sanity-blog/source",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "predev:all": "npm run typegen",
    "dev:all": "nx run-many --target=serve --all --parallel",
    "prebuild": "npm run typegen",
    "build": "nx run-many --target=build --all --parallel",
    "typegen": "node run-npx.mjs -w studio sanity schema extract && node run-npx.mjs -w studio sanity typegen generate",
    "presetup": "echo 'about to setup env variables, follow the guide here: https://github.com/osnoser1/analog-sanity-blog/?tab=readme-ov-file#using-the-sanity-cli'",
    "setup": "npx sanity@latest init --env .env.local"
  },
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "dependencies": {
    "date-fns": "^3.6.0",
    "groq": "^3.53.0",
    "marked-highlight": "^2.1.4",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@analogjs/content": "1.9.2",
    "@analogjs/platform": "1.9.2",
    "@analogjs/vite-plugin-angular": "1.9.2",
    "@angular-devkit/schematics": "^18.0.0",
    "@angular/build": "^18.0.0",
    "@angular/compiler-cli": "^18.0.0",
    "@eslint/eslintrc": "^2.1.1",
    "@eslint/js": "^9.11.0",
    "@nx/angular": "20.0.12",
    "@nx/devkit": "20.0.12",
    "@nx/eslint": "20.0.12",
    "@nx/eslint-plugin": "20.0.12",
    "@nx/vite": "20.0.12",
    "@nx/workspace": "20.0.12",
    "@schematics/angular": "^18.0.0",
    "@tailwindcss/typography": "^0.5.14",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@typescript-eslint/utils": "^8.6.0",
    "angular-eslint": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "20.0.12",
    "postcss": "^8.4.5",
    "postcss-url": "~10.1.3",
    "prettier": "3.3.3",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "tailwindcss": "^3.0.2",
    "typescript": "~5.4.2",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.0.0",
    "vite-plugin-webfont-dl": "^3.9.4",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
